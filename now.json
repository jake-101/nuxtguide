{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder",
      "config": {
        "serverFiles": ["server-middleware/**"]
      }
    }
  ],
  "routes": [
    {
      "status": 200,
      "src": "/api/(.*)",
      "dest": "http://13.66.135.133:7000/_/$1",
      "headers": { "Cache-Control": "s-max-age=3600,stale-while-revalidate" }
    },
    { "src": "/_nuxt/.+", "headers": { "Cache-Control": "max-age=31557600" } },
    { "src": "/(.*)", "dest": "/" }
  ]
}
